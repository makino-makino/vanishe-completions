{
  "name": "vanishe completions",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "あなたの心のばにしぇを補完します",
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": [
        "config/urls.js",
        "config/words.js",
        "lib/axios.js",
        "js/axios.js",
        "js/io.js",
        "js/diff.js",
        "js/henkan.js",
        "js/update.js",
        "js/completion.js",
        "js/content.js"
      ]
    }
  ],
  "permissions": ["storage"]
}
